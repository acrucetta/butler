{
  "profiles": [
    {
      "id": "primary-openrouter",
      "provider": "openrouter",
      "model": "moonshotai/kimi-k2.5",
      "cooldownSeconds": 180
    },
    {
      "id": "fallback-openai",
      "provider": "openai",
      "model": "gpt-4.1-mini",
      "cooldownSeconds": 120,
      "envFrom": {
        "OPENAI_API_KEY": "OPENAI_API_KEY"
      }
    },
    {
      "id": "backup-openrouter",
      "provider": "openrouter",
      "model": "openai/gpt-4.1-mini",
      "cooldownSeconds": 180,
      "envFrom": {
        "OPENROUTER_API_KEY": "OPENROUTER_API_KEY_BACKUP"
      }
    }
  ],
  "routes": {
    "default": [
      "primary-openrouter",
      "fallback-openai",
      "backup-openrouter"
    ],
    "task": [
      "primary-openrouter",
      "fallback-openai"
    ],
    "run": [
      "primary-openrouter",
      "backup-openrouter"
    ]
  },
  "maxAttemptsPerJob": 3,
  "retryableErrorPatterns": [
    "rate limit",
    "timeout",
    "429",
    "503",
    "authentication",
    "api key"
  ]
}
